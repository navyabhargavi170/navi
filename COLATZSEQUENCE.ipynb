    "def colatz(num):\n",
    "    while (num!=1):\n",
    "        print(num)\n",
    "        if num%2==0:\n",
    "            num = num//2\n",
    "        else:\n",
    "            num=3*num+1\n",
    "    print(num)\n",
    "        \n",
    "colatz(3)"
